@namespace TDesignBlazor._6x.Component
@using TDesignBlazor._6x.Core.Base
@inherits TDesignComponentBase
@(new MarkupString(IconStore.GetIcon(this)))

@code {
    [Parameter]
    public bool LoadDefaultIcons { get; set; } = true;
    [Parameter]
    public string Name { get; set; }
    [Parameter]
    public string Size { get; set; } = "undefined";
    [Parameter]
    public string Style { get; set; }
    [Parameter]
    public string Url { get; set; }
    [Parameter]
    public Action OnClick { get; set; }
    [Parameter]
    public RenderFragment ChildContent { get; set; }

}
